{
  "manifest_version": 3,
  "name": "easYupoo",
  "author": "1etu",
  "version": "1.0",
  "description": "A Google Extension to make Yupoo searching easier.",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "https://www.jadeship.com/*",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "host_permissions": [
    "*://*.yupoo.com/*",
    "*://*.jadeship.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["*://*.yupoo.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources": ["static/jadeship.png"],
    "matches": ["<all_urls>"]
  }]
}
